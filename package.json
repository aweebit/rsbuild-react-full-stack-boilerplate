{
  "name": "@aweebit/rsbuild-react-full-stack-boilerplate",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "client": "rsbuild --config rsbuild/client.config.ts",
    "server:rsbuild": "rsbuild --config rsbuild/server.config.ts",
    "server:node": "node --watch server/dist/main.cjs",
    "server": "concurrently -n rsbuild,node -c green,magenta \"npm run server:rsbuild\" \"npm run server:node\"",
    "dev": "concurrently -n rsbuild,node -c green,magenta \"rsbuild\" \"npm run server:node\"",
    "lint": "eslint --fix --max-warnings=0",
    "prettier": "prettier --write .",
    "build:client": "rsbuild build --config rsbuild/client.config.ts",
    "build:server": "rsbuild build --config rsbuild/server.config.ts",
    "build": "rsbuild build",
    "preview": "rsbuild preview",
    "start": "node server/dist/main.cjs"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@rsbuild/core": "^1.4.7",
    "@rsbuild/plugin-react": "^1.3.4",
    "@rsbuild/plugin-type-check": "^1.2.3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.16.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "concurrently": "^9.2.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "prettier": "3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.37.0"
  },
  "dependencies": {
    "@aweebit/react-essentials": "^0.5.4",
    "arktype": "^2.1.20",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
